{
  "name": "@zeainc/zea-pointclouds",
  "filename": "zea-pointclouds.js",
  "library": "ZeaPointClouds",
  "private": false,
  "version": "1.3.0",
  "description": "Large point cloud rendering extension for Zea Engine",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "rawimport": "dist/index.rawimport.js",
  "files": [
    "/dist/index.cjs.js",
    "/dist/index.esm.js",
    "/dist/index.rawimport.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepare": "yarn build",
    "start": "rollup -c rollup.config.dev.js -w",
    "dist": "yarn publish --access=public",
    "docs": "adg --config adg.config.json",
    "docs-w": "adg -w --config=adg.config.json",
    "testing-server": "node testing-server/index.js",
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "testBrowser": "karma start --single-run karma.conf.js"
  },
  "dependencies": {
    "@zeainc/zea-engine": "^1.0.7",
    "binaryheap": "^0.0.3",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-util": "^3.0.8",
    "through": "~2.3.4",
    "webpack": "^4.41.2",
    "worker-loader": "^2.0.0"
  },
  "author": "Philip Taylor",
  "license": "BSD-2-CLAUSE",
  "devDependencies": {
    "@zeainc/jsdocs2md": "^0.0.3",
    "eslint": "^7.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "rollup": "^2.10.9",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-web-worker-loader": "^1.3.0",
    "webpack-cli": "^3.3.9"
  }
}
